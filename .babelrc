{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": "> 0.25%, not dead",
                "useBuiltIns": "usage",
                "corejs": 3
            }
        ]
    ],
    "plugins": [
        [
            "@vue/babel-plugin-jsx",
            {
                "mergeProps": false
            }
        ],
        [
            "@babel/plugin-transform-runtime",
            {
                "proposals": true,
                "corejs": 3
            }
        ]
    ]
}